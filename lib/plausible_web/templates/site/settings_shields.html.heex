<%= case @shield do %>
  <% "ip_addresses" -> %>
    <%= live_render(@conn, PlausibleWeb.Live.Shields.Tabs,
      session: %{
        "site_id" => @site.id,
        "domain" => @site.domain,
        "remote_ip" => PlausibleWeb.RemoteIP.get(@conn)
      }
    ) %>
  <% "countries" -> %>
    <%= "test" %>
<% end %>
