  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "strict": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react",
    "forceConsistentCasingInFileNames": true
  },
  "include": ["js", "css"],
  "exclude": ["node_modules"]
}
